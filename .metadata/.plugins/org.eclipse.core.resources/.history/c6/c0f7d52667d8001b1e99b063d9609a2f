package sisFrases.Model;

public class Livro {
	
	private Integer livroId;
	private String livroTitulo;
	private Autor livroAutor;

	public Livro() {
		
	}
	
	public Livro(Integer livroId, String titulo, Autor autor) {
		this.livroAutor = autor;
		this.livroId = livroId;
		this.livroTitulo = titulo;
	}

	public Integer getLivroId() {
		return livroId;
	}

	public void setLivroId(Integer livroId) {
		this.livroId = livroId;
	}

	public String getTitulo() {
		return livroTitulo;
	}

	public void setTitulo(String titulo) {
		this.livroTitulo = titulo;
	}

	public Autor getAutor() {
		return livroAutor;
	}

	public void setAutor(Autor autor) {
		this.livroAutor = autor;
	}

	@Override
	public String toString() {
		return "Livro [livroId=" + livroId + ", titulo=" + livroTitulo + ", autor=" + livroAutor + "]";
	}
	
	
}
